{
  "name": "env-type-generator",
  "version": "1.0.1",
  "description": "Zero-config TypeScript type generator for .env files with Zod validation, watch mode, and IDE autocomplete support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "env-type-gen": "dist/cli.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!dist/**/*.test.js",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run format:check && npm run test && npm run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "env",
    "dotenv",
    "environment-variables",
    "type-generation",
    "type-safety",
    "typescript-types",
    "code-generation",
    "codegen",
    "validation",
    "zod",
    "zod-schema",
    "runtime-validation",
    "process-env",
    "autocomplete",
    "intellisense",
    "vscode",
    "watch-mode",
    "cli",
    "developer-tools",
    "devtools",
    "dx",
    "developer-experience",
    "nodejs",
    "node",
    "next",
    "nextjs",
    "react",
    "express",
    "nestjs",
    "config",
    "configuration",
    "env-file",
    "dotenv-safe",
    "type-checking"
  ],
  "author": "Ashish Yadav <ashish.yd@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashishyd/env-type-generator.git"
  },
  "bugs": {
    "url": "https://github.com/ashishyd/env-type-generator/issues"
  },
  "homepage": "https://github.com/ashishyd/env-type-generator#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  }
}